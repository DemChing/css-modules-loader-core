{
  "name": "@demching113/css-modules-loader-core",
  "version": "1.1.0",
  "description": "A loader-agnostic CSS Modules implementation, based on PostCSS@8.x",
  "main": "lib/index.js",
  "directories": {
    "test": "test"
  },
  "dependencies": {
    "icss-replace-symbols": "1.1.0",
    "postcss": "8.4.12",
    "postcss-modules-extract-imports": "3.0.0",
    "postcss-modules-local-by-default": "4.0.0",
    "postcss-modules-scope": "3.0.0",
    "postcss-modules-values": "4.0.0"
  },
  "devDependencies": {
    "@babel/cli": "^7.17.6",
    "@babel/core": "^7.17.8",
    "@babel/preset-env": "^7.16.11",
    "@babel/preset-typescript": "^7.16.7",
    "@types/chai": "^4.3.0",
    "@types/mocha": "^9.1.0",
    "@types/node": "^17.0.21",
    "babelify": "10.0.0",
    "chokidar-cli": "1.1.0",
    "eslint": "3.10.1",
    "mocha": "3.1.2",
    "ts-node": "^10.7.0",
    "typescript": "^4.6.2"
  },
  "scripts": {
    "lint": "eslint src",
    "build": "babel --extensions '.ts' --out-dir lib src && tsc",
    "autotest": "chokidar src test -c 'npm test'",
    "test": "mocha --compilers ts:ts-node/register",
    "prepublish": "rm -rf lib/* && npm run build"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/demching/css-modules-loader-core.git"
  },
  "keywords": [
    "css-modules",
    "postcss",
    "loader"
  ],
  "files": [
    "lib"
  ],
  "author": "Dem Ching",
  "license": "ISC",
  "bugs": {
    "url": "https://github.com/demching/css-modules-loader-core/issues"
  },
  "homepage": "https://github.com/demching/css-modules-loader-core",
  "publishConfig": {
    "access": "public"
  }
}
